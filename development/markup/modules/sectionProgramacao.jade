mixin sectionProgramacao()
	.row
		.title-section
			h2="Programação"
		ul.timeline
			li
				.box-talk
					time.talk-time="08:00 - 8:40"
					h4.sub-title="Credenciamento"
					p="Não esqueça de trazer seu RG para a identificação e entrega de brindes."
			li
				.box-talk
					time.talk-time="08:40 - 9:20"
					h4.sub-title="Coffee Break"
					p="Antes do evento começar, aproveite para tomar um café, fazer novas amizades e rever os amigos."
			li.li-speaker
				.box-speaker
					.box-speaker-media
						img(alt='Foto do Ciro Nunes', src='assets/image/ciro.jpg')
					|     
					h3.sub-title Ciro Nunes - apresentador
					|     
					p
						| Senior Front-end Engineer at 
						a(href='http://www.questrade.com', target='_blank') Questrade
					|     
					p
						i.icon-social-twitter
						|       
						a.hover.hover-3d.speaker-link(href='https://twitter.com/cironunesdev', target='_blank')
							span(data-title='@cironunesdev') @cironunesdev
				.box-talk
					time.talk-time="9:20 - 9:30"
					h4.sub-title="Abertura e apresentações com Pedro Polisenso e André Vitor"
					p="Nosso amigo Ciro Nunes vai conduzir o evento até o final, apresentando cada palestrante, batendo um papo com eles e o resto você acompanha de perto."
			
			
			li.li-speaker
				.box-speaker
					.box-speaker-media
						img(alt='Foto do Vitor Mattos', src='assets/image/vitor.png')
