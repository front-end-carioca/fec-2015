mixin header(title)
	header(role="banner", data-parallax="scroll")
		.wrapper-intro(data-stellar-ratio="0.9")
			.intro-logo.animated-logo
				img(src="assets/image/logo.png" alt="Logo Front End Carioca 2015")
				h1="FRONT END CARIOCA"
			.row
				.intro-datails.animated-datails
					span.intro-datails_text
						time(class="intro-datails_time")="26 de Setembro, 2015"
						|  - Rio de Janeiro / Barra da Tijuca
					<br>
					span.intro-datails_text.sorteio="Os ingressos acabaram, inscreva-se na fila de espera."
				.intro-actions
					a(href="mailto:contato@frontendcarioca.com.br?subject=PATROCINIO - FRONT END CARIOCA 2015", title="Seja um Patrocinador", class="animated-btns-left")="SEJA UM PATROCINADOR"
					a(href="http://even.tc/front-end-carioca-fila-de-espe", title="Inscreva-se" class="animated-btns-right", target="_blank", onClick="_gaq.push(['_trackEvent', 'Links', 'Click', 'Inscreva-se'])")="LOTE EXTRA"